{
  "name": "onsuperfit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "deploy-prod": "npm run swap-prod && ng build --aot --prod && firebase use production && firebase deploy --only hosting",
    "deploy-staging": "npm run swap-staging && ng build --configuration=staging && firebase use staging && firebase deploy --only hosting",
    "swap-prod": "cp prod.firebase.json firebase.json",
    "swap-staging": "cp staging.firebase.json firebase.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run onsuperfit:serve-ssr",
    "serve:ssr": "node dist/onsuperfit/server/main.js --trace-warnings --trace-deprecation",
    "build:ssr": "ng build --prod && ng run onsuperfit:server:production",
    "prerender": "ng run onsuperfit:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.4",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/fire": "^6.0.0-rc.1",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/platform-server": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@nguniversal/express-engine": "^9.0.2",
    "@superfitapp/superfitjs": "2.1.1-beta.4",
    "angulartics2": "^8.3.0",
    "branch-sdk": "^2.52.3",
    "compression": "^1.7.4",
    "core-js": "^2.6.11",
    "domino": "^2.1.5",
    "express": "^4.15.2",
    "firebase": "^7.8.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.900.0-0 || ^0.900.0",
    "@angular-devkit/build-angular": "^0.900.1",
    "@angular/cli": "^9.0.0",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@nguniversal/builders": "^9.0.2",
    "@types/auth0-js": "^8.11.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^8.10.59",
    "codelyzer": "^5.2.1",
    "firebase-tools": "^7.12.1",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "^3.7.5"
  },
