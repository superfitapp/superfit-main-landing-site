{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "superfit-landing-website": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": [
              "src/assets/js/scrollMonitor.js",
              "src/assets/js/jquery.min.js",
              "src/assets/js/jquery.fancybox.min.js",
              "src/assets/js/jquery.countdown.min.js",
              "src/assets/js/jquery.smartWizard.min.js",
              "src/assets/js/aos.js",
              "src/assets/js/svg-injector.umd.production.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/bootstrap.js",
              "src/assets/js/flatpickr.min.js",
              "src/assets/js/clipboard.js",
              "src/assets/js/flickity.pkgd.min.js",
              "src/assets/js/ion.rangeSlider.min.js",
              "src/assets/js/isotope.pkgd.min.js",
              "src/assets/js/jarallax.min.js",
              "src/assets/js/jarallax-video.min.js",
              "src/assets/js/jarallax-element.min.js",
              "src/assets/js/prism.js",
              "src/assets/js/smooth-scroll.polyfills.min.js",
              "src/assets/js/twitterFetcher_min.js",
              "src/assets/js/typed.min.js",
              "src/assets/js/theme.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "superfit-landing-website:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "superfit-landing-website:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "superfit-landing-website:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "superfit-landing-website-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "superfit-landing-website:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "superfit-landing-website:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "superfit-landing-website"
}